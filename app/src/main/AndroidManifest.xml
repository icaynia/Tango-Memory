<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icaynia.tangomemory">
    <uses-sdk
        android:minSdkVersion="7"
        android:maxSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.icaynia.tangomemory.Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.icaynia.tangomemory.MainActivity" />
        <activity android:name="com.icaynia.tangomemory.infActivity" />
        <activity android:name="com.icaynia.tangomemory.wordtest.WordToHiragana"
            android:screenOrientation="portrait"></activity>

        <activity android:name="com.icaynia.tangomemory.TestActivity"
            android:theme="@style/Theme.MyApp"/>



    </application>

</manifest>